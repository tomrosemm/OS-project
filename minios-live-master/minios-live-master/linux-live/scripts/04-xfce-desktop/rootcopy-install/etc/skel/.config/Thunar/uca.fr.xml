<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<icon>utilities-terminal</icon>
		<name>Ouvrir Terminal Ici</name>
		<submenu></submenu>
		<unique-id>1713380276737258-1</unique-id>
		<command>exo-open --working-directory %f --launch TerminalEmulator</command>
		<description>Ouvrir le terminal dans le répertoire actuel</description>
		<range></range>
		<patterns>*</patterns>
		<startup-notify/>
		<directories/>
	</action>
	<action>
		<icon>module-connect</icon>
		<name>Activer Module</name>
		<submenu></submenu>
		<unique-id>1713385765967387-1</unique-id>
		<command>pkexec /usr/bin/sb activate %f</command>
		<description>Activer le module spécifié</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<other-files/>
	</action>
	<action>
		<icon>module-disconnect</icon>
		<name>Désactiver Module</name>
		<submenu></submenu>
		<unique-id>1713385872440460-2</unique-id>
		<command>pkexec /usr/bin/sb deactivate %f</command>
		<description>Désactiver le module spécifié</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<other-files/>
	</action>
	<action>
		<icon>module-create</icon>
		<name>Créer Module</name>
		<submenu></submenu>
		<unique-id>1713386118612735-3</unique-id>
		<command>/usr/bin/dir2sb %f</command>
		<description>Créer un module à partir du répertoire spécifié</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<directories/>
	</action>
	<action>
		<icon>module-extract</icon>
		<name>Extraire Module</name>
		<submenu></submenu>
		<unique-id>1713386196741360-4</unique-id>
		<command>/usr/bin/sb2dir %f</command>
		<description>Extraire le module spécifié</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<other-files/>
	</action>
</actions>
