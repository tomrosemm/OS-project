<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<icon>utilities-terminal</icon>
		<name>Apri Terminale Qui</name>
		<submenu></submenu>
		<unique-id>1713380276737258-1</unique-id>
		<command>exo-open --working-directory %f --launch TerminalEmulator</command>
		<description>Apri il terminale nella directory corrente</description>
		<range></range>
		<patterns>*</patterns>
		<startup-notify/>
		<directories/>
	</action>
	<action>
		<icon>module-connect</icon>
		<name>Attiva Modulo</name>
		<submenu></submenu>
		<unique-id>1713385765967387-1</unique-id>
		<command>pkexec /usr/bin/sb activate %f</command>
		<description>Attiva il modulo specificato</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<other-files/>
	</action>
	<action>
		<icon>module-disconnect</icon>
		<name>Disattiva Modulo</name>
		<submenu></submenu>
		<unique-id>1713385872440460-2</unique-id>
		<command>pkexec /usr/bin/sb deactivate %f</command>
		<description>Disattiva il modulo specificato</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<other-files/>
	</action>
	<action>
		<icon>module-create</icon>
		<name>Crea Modulo</name>
		<submenu></submenu>
		<unique-id>1713386118612735-3</unique-id>
		<command>/usr/bin/dir2sb %f</command>
		<description>Crea un modulo dalla directory specificata</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<directories/>
	</action>
	<action>
		<icon>module-extract</icon>
		<name>Estrai Modulo</name>
		<submenu></submenu>
		<unique-id>1713386196741360-4</unique-id>
		<command>/usr/bin/sb2dir %f</command>
		<description>Estrai il modulo specificato</description>
		<range>*</range>
		<patterns>*.sb</patterns>
		<other-files/>
	</action>
</actions>
